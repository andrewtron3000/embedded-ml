let 
  import "std.uh"
  import "string.uh"
  import "list.uh"
  import "int.uh"
  import "threads.uh"
  import "queues.uh"
  import "tasks.uh"

  val q = queue-create ()
          
  val a-queue = queue-send q "hihi"

  fun terminal-hook name m = ( print [[name](terminal): message is [m]\n] ;
                               name )
                             
  val () = task-create a-queue terminal-hook "taskA"
           
  fun idle () = ( yield () ; 
                  idle () )
in
    idle ()
end
